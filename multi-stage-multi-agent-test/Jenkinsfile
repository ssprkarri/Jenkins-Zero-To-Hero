pipeline {
  agent {
    docker { image 'maven:3.8.1-adoptopenjdk-11' }
  }
  stages {
    stage('Checkout') {
        steps {
            git branch: 'main', url: 'https://github.com/ssprkarri/Jenkins-Zero-To-Hero' 
        }
    }
    stage('Build and Run') {
        steps {
            script {
                // Build and run java code
                sh 'javac HelloWorld.java'
                sh 'java HelloWorld'
                }
            }
    }
  }
}
